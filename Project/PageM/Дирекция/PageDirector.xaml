<Page x:Class="Project.PageM.Дирекция.PageDirector"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project.PageM.Дирекция"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageDirector">

    <Grid>
        <StackPanel>
            <Button
            Height="30"
            Name="PageAccountingMaterials"
            Click="PageAccountingMaterials_Click"
            Width="150"
            Content="PageAccountingMaterials"/>
            <Button
            Height="30"
            Width="150"
            Name="PageConstructorProducts"
            Click="PageConstructorProducts_Click"
            Content="PageConstructorProducts"/>
            <Button
            Height="30"
            Width="150"
            Content="PageConstructorProducts"/>
        </StackPanel>

    </Grid>
</Page>
