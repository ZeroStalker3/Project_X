<Page x:Class="Project.PageM.Менеджер.PageMenedger"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project.PageM.Менеджер"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Страница менеджера">

    <Grid>
        <TabControl Grid.Row="1">
            <TabItem Header="Заказы">
                <Frame Name="FrOrders" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem Header="Список изделий">
                <Frame Grid.Row="1" Name="FrProductList" NavigationUIVisibility="Hidden"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
